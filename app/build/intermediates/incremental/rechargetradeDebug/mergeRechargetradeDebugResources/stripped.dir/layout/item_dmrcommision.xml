<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/lyt_parent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/dp_10"
    android:background="@drawable/back_dash_menuone"
    android:orientation="horizontal">

    <LinearLayout
        android:id="@+id/lay_head_change"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="@dimen/ten">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="Start Range"
                android:textColor="@color/et_textcolor"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/startrange_commision"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/et_textcolor"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="End Range"
                android:textColor="@color/et_textcolor"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/endrange_commision"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/et_textcolor"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="Surcharge"
                android:textColor="@color/et_textcolor"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/surcharge_dmr_commision"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/et_textcolor"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="Is Flat?"
                android:textColor="@color/et_textcolor"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/flat_dmr_commision"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/et_textcolor"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>





    </LinearLayout>

</LinearLayout>
