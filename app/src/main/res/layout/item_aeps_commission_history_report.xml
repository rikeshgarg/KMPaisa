<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/lyt_parent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/back_dash_menuone"
    android:orientation="horizontal"
    android:layout_marginBottom="@dimen/dp_10"
    >

    <LinearLayout
        android:layout_margin="@dimen/five"
        android:id="@+id/lay_head_change"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="@dimen/ten">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="Member Id"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_member_id"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="Name"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="Transaction Id"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_trnsc_id"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="Transaction Type"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_trnsc_type"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="Amount"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_amount"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="Commission"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_commission"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="Type"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_type"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/two"
            android:orientation="horizontal"
            android:weightSum="10">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="4"
                android:text="Date Time"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_forteen"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_datetime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_weight="6"
                android:textColor="@color/fontcolortextview"
                android:textSize="@dimen/ts_twelve"
                android:textStyle="bold" />
        </LinearLayout>


    </LinearLayout>

</LinearLayout>
